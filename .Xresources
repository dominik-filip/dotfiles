! terminal colors ----------------------------------

! tangoesque scheme
*background: #111111
*foreground: #dcdccc
! Black (not tango) + DarkGrey
*color0: #000000
*color8: #555753
! DarkRed + Red
*color1: #ff6565
*color9: #ff8d8d
! DarkGreen + Green
*color2: #93d44f
*color10: #c8e7a8
! LightGrey + White
*color7: #cccccc
*color15: #ffffff

! fonts.conf recommendations
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.autohint: false

! Disable ctrl+shift keycap
URxvt*iso14755: false
URxvt*iso14755_52: false

! URxvt.keysym.Control-Tab: \033[27;5;9~

URxvt.scrollBar: false
URxvt.letterSpace: -1
URxvt.font: xft:DejaVu Sans Mono:size=11
! URxvt.font: -*-terminus-medium-*-*-*-18-*-*-*-*-*-*-*
URxvt*loginShell: true

! Disable Perl extensions for security and speed
URxvt.perl-ext:
URxvt.perl-ext-common:
